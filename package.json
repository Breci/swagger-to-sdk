{
  "name": "swagger-to-sdk",
  "version": "0.0.10",
  "description": "Convert a swagger to JavaScript SDK",
  "source": "src/index.js",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "scripts": {
    "build": "rimraf ./dist  && microbundle && copyfiles -f ./src/templates/*.ejs dist/templates",
    "start": "node ./src/index.js -o ./test"
  },
  "author": "Breci",
  "license": "ISC",
  "devDependencies": {
    "copyfiles": "^2.3.0",
    "microbundle": "^0.12.3",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "fs-extra": "^9.0.1",
    "ejs": "^3.1.3",
    "minimist": "^1.2.5",
    "node-fetch": "^2.6.0"
  },
  "files": [
    "dist/index.js",
    "dist/index.map.js",
    "dist/templates/*"
  ],
  "homepage": "https://github.com/Breci/swagger-to-sdk",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/Breci/swagger-to-sdk"
  }
}
